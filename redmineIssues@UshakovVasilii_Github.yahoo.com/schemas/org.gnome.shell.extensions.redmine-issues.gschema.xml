<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="org.gnome.shell.extensions.redmine-issues" path="/org/gnome/shell/extensions/redmine-issues/">
		<key name="redmine-url" type="s">
			<default>'http://'</default>
			<summary>Readmine URL</summary>
		</key>
		<key name="api-access-key" type="s">
			<default>''</default>
			<summary>API access key</summary>
		</key>
		<key name="group-by" type="s">
			<default>'project'</default>
			<summary>Group By</summary>
		</key>
		<key name="auto-refresh" type="i">
			<default>30</default>
			<summary>Auto refresh</summary>
		</key>
		<key name="show-status-item-status" type="b">
			<default>true</default>
			<summary>Show Status</summary>
		</key>
		<key name="show-status-item-tracker" type="b">
			<default>false</default>
			<summary>Show Tracker</summary>
		</key>
		<key name="show-status-item-done-ratio" type="b">
			<default>false</default>
			<summary>Show Done Ratio</summary>
		</key>
		<key name="show-status-item-author" type="b">
			<default>false</default>
			<summary>Show Author</summary>
		</key>
		<key name="show-status-item-project" type="b">
			<default>false</default>
			<summary>Show Project</summary>
		</key>
		<key name="show-status-item-assigned-to" type="b">
			<default>true</default>
			<summary>Show Assigned To</summary>
		</key>
		<key name="show-status-item-priority" type="b">
			<default>false</default>
			<summary>Show Priority</summary>
		</key>
		<key name="show-status-item-fixed-version" type="b">
			<default>false</default>
			<summary>Show Target Version</summary>
		</key>
		<key name="show-status-item-category" type="b">
			<default>false</default>
			<summary>Show Category</summary>
		</key>
		<key name="max-subject-width" type="i">
			<default>400</default>
			<summary>Max Subject Width</summary>
		</key>
		<key name="min-menu-item-width" type="i">
			<default>600</default>
			<summary>Min Menu Item Width</summary>
		</key>
		<key name="filters" type="as">
			<default>[]</default>
			<summary>Filters</summary>
		</key>
	</schema>
</schemalist>
